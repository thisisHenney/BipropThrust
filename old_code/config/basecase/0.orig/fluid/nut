FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 9.78e-5;

boundaryField
{
    "(fluid_to_.*)"
    {
        type            nutUSpaldingWallFunction;        
        value           $internalField;
    }

    "(min.*|max.*)"
    {
        type            zeroGradient;
    }
}


