{
    fluid_to_header
    {
        inGroups        1(wall);
    }

    fluid_to_throatPtRh
    {
        inGroups        1(wall);
    }

    fluid_to_ccNimonic
    {
        sampleRegion    ccNimonic;
        samplePatch     ccNimonic_to_fluid;
    }
}



