FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

TNoz  300;
TInj  330;

pNoz  25000;
pInj  1e5;

defaultFieldValues
(
    volScalarFieldValue T  $TNoz
    volScalarFieldValue p  $pNoz
);

regions
(
    cylinderToCell
    {
        p1 (-0.002 0 0);
        p2 ( 0.018 0 0);  //not include throat
//        p2 ( 0.022 0 0);  //include throat        
        radius 0.004;
        fieldValues
        (
            volScalarFieldValue T  $TInj
            volScalarFieldValue p  $pInj
        );
    }
);


