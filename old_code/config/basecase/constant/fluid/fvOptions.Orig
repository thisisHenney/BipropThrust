FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvOptions;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

pressureIgnition
{
    type            scalarFixedValueConstraint;

    timeStart       0.0;
    duration        0.0005;
    selectionMode   cellSet;
    cellSet         ignition;
    fieldValues
    {
        p           1e5;
    }
}

