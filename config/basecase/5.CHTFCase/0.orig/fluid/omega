FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 1.93e+4;

boundaryField
{
    "(fluid_to_.*)"
    {
        type            omegaWallFunction;
        value           $internalField;
    }
    
    outlet
    {
        type            zeroGradient;
    }

}


