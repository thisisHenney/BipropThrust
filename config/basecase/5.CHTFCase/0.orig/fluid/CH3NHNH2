FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      CH3NHNH2;
}
// ************************************************************************* //

dimensions          [0 0 0 0 0 0 0];

internalField       uniform 1.0;

boundaryField
{
    "(fluid_to_.*)"
    {
        type            zeroGradient;
    }

    outlet
    {
        type            zeroGradient;
    }       
}


